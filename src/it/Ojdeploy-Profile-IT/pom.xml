<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>

  <groupId>com.github.badamowicz</groupId>
  <artifactId>Ojdeploy-Profile-IT</artifactId>
  <version>@project.version@</version>
  <packaging>pom</packaging>

  <description>
    IT for executing ojdeploy.
  </description>

  <name>Ojdeploy-Profile-IT</name>


<!-- /opt/Oracle/Middleware/jdeveloper/jdev/bin/ojdeploy -profile ShuttleDemo_application1 -workspace /home/bernd/THEMEN/BA/DEV/maven-ojdeploy-plugin/target/it/Ojdeploy-Profile-IT/ShuttleDemo/ShuttleDemo.jws -basedir /home/bernd/THEMEN/BA/DEV/maven-ojdeploy-plugin/target/it/Ojdeploy-Profile-IT  -statuslogfile /home/bernd/THEMEN/BA/DEV/maven-ojdeploy-plugin/target/it/Ojdeploy-Profile-IT/status.xml -timeout 30000 -->

  <build>
    <plugins>
      <plugin>
        <groupId>com.github.badamowicz</groupId>
        <artifactId>ojdeploy-maven-plugin</artifactId>
        <version>@project.version@</version>
        <configuration>
          <dryRun>false</dryRun>
          <jdevBinPath>/opt/Oracle/Middleware/jdeveloper/jdev/bin</jdevBinPath>
          <workspaceFile>ShuttleDemo/ShuttleDemo.jws</workspaceFile>
          <!-- <buildFile></buildFile> -->
          <outputFile></outputFile>
          <profile>ShuttleDemo_application1</profile>
          <!-- <project></project> -->
          <!-- <buildFileSchema></buildFileSchema> -->
          <baseDir>.</baseDir>
          <nocompile>false</nocompile>
          <nodependents>false</nodependents>
          <clean>false</clean>
          <nodatasources>false</nodatasources>
          <forceRewrite>false</forceRewrite>
          <updateWebxmlEJBRefs>false</updateWebxmlEJBRefs>
          <defines></defines>
          <statusLogFile>status.xml</statusLogFile>
          <timeout></timeout>
        </configuration>
        <executions>
          <execution>
            <id>execute</id>
            <goals>
              <goal>ojdeploy</goal>
            </goals>
            <phase>compile</phase>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>

</project>
